<%_ include( '_const') _%>
<!DOCTYPE html>
<html lang="ja">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no">
  <title><% if( data.title !== '' ){ %><%= data.title + ' | ' ; %><%= TITLE %><% }else{ %><%= TITLE %><% } %></title>
  <link rel="canonical" href="<% if( data.url !== '' ){ %><%= data.url; %><% }else{ %><%= SITEURL %><% } %>">
  <meta name="description" content="<% if( data.description !== '' ){ %><%= data.description; %><% }else{ %><%= DESCRIPTION %><% } %>" />
  <meta name="keywords" content="<% if( data.keyword !== '' ){ %><%= data.keyword + ','; %><%= KEYWORD %><% }else{ %><%= KEYWORD %><% } %>" />
  <link rel="stylesheet" href="<%= data.current %>assets/css/style.css?<%= DATE %>">
</head>
<body id="<%= data.name %>">
<%- include( '_header_' + data.device , { data : data } ); %>
